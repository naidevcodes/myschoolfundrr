<%= form_for(@fundrr) do |f| %>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>
  <div class="field">
    <%= f.label :start_date %><br>
    <%= f.text_field :start_date %>
  </div>
 <div class="field">
   <%= f.label :duration %><br>
   <%= f.text_field :duration %>
 </div>
 <div class="field">
   <%= f.label :fundrr_goal %><br>
   <%= f.text_field :fundrr_goal %>
 </div>
  
  <div class="actions">
    <%= f.submit %>
  </div>
  
<% end %>